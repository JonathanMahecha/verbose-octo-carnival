Random random = new Random();

        for (int i = 0; i < 10; i++) {
            int num1 = random.nextInt(100) + 1; // 1 al 100
            int num2 = random.nextInt(100) + 1;
            int num3 = random.nextInt(10) + 1;  // 1 al 10

            String direccion = "CLL " + num1 + " " + num2 + " " + num3;
            System.out.println(direccion);
        }
