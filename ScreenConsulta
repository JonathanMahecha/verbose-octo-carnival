	public String aprobarSolicitudSuperior(String tipoId, String numeroId, String tipoProducto, String cupo,
			String oficina) throws Exception {
		selectItemListaConFiltro("110", 'X', 12, 3);
		while (true) {
			Util.wait(1);
			if (read().contains("APROBACION DE SOLICITUDES")) {
				write(numeroId);
				tab(3);
				write(tipoProducto);
				tab(1);
				write(cupo);
				tab(1);
				enter();
				tab(7);
				write("A");
				enter();
				while (true) {
					Util.wait(1);
					if (read().contains("CONFIRM")) {
						Evidence.save("Aprobar Cliente: " + numeroId, (BaseWindowsApp) this);
						sendKey('Y');
						while (true) {
							Util.wait(1);
							if (read().contains("La Aprobacide la Solicitud")) {
								sendFunction("F3");
								consultaPrimeraVez(tipoId, numeroId, tipoProducto, oficina);
								return "";
							}
						}
					}
				}
			}
		}
	}

	public String aprobarSolicitudIntermedio(String tipoId, String numeroId, String tipoProducto, String cupo,
			String oficina) throws Exception {
