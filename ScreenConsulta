				write(tipoProducto);
				tab(1);
				write(cupo);
				tab(1);
				enter();
				tab(7);
				write("A");
				enter();
				while (true) {
					Util.wait(1);
					if (read().contains("CONFIRM")) {
						Evidence.save("Aprobar Cliente: " + numeroId, (BaseWindowsApp) this);
						sendKey('Y');
						while (true) {
							Util.wait(1);
							if (read().contains("La Aprobaci√≥n de la Solicitud")) {
								sendFunction("F3");
								consultaPrimeraVez(tipoId, numeroId, tipoProducto, oficina);
								return "";
							}
						}
					}
				}
			}
		}
